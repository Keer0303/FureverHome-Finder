{"ast":null,"code":"export default {\n  name: \"Submit\",\n  data() {\n    return {\n      tableData: [],\n      // 表格数据\n      pageNum: 1,\n      // 当前页码\n      pageSize: 10,\n      // 每页显示条数\n      total: 0,\n      // 总条数\n      searchForm: {\n        start: null,\n        // 开始日期\n        end: null,\n        // 结束日期\n        status: null // 状态\n      },\n\n      formVisible: false,\n      // 表单是否可见\n      form: {},\n      // 表单数据\n      user: JSON.parse(localStorage.getItem('xm-user') || '{}'),\n      // 用户信息\n      rules: {\n        name: [{\n          required: true,\n          message: 'Please enter the description',\n          trigger: 'blur'\n        }]\n      }\n    };\n  },\n  startPickerOptions: {\n    disabledDate(time) {\n      return time.getTime() < startDate.getTime() || time.getTime() > currentDate.getTime();\n    }\n  },\n  endPickerOptions: {\n    disabledDate(time) {\n      return time.getTime() < startDate.getTime() || time.getTime() > currentDate.getTime();\n    }\n  },\n  created() {\n    this.load(1);\n  },\n  methods: {\n    handleAdd() {\n      // 添加新数据\n      this.form = {\n        status: 'Pending'\n      }; // 清空表单数据\n      this.formVisible = true; // 打开对话框\n    },\n\n    deal(row, status) {\n      let data = {\n        ...row,\n        status\n      }; // 更新数据的状态\n      this.$request.put('/submit/update', data).then(res => {\n        if (res.code === '200') {\n          this.$message.success('Operation successful');\n          this.load(1);\n        } else {\n          this.$message.error(res.msg);\n        }\n      });\n    },\n    save() {\n      // 保存表单数据\n      this.$refs.formRef.validate(valid => {\n        if (valid) {\n          this.$request({\n            url: this.form.id ? '/submit/update' : '/submit/add',\n            method: this.form.id ? 'PUT' : 'POST',\n            data: this.form\n          }).then(res => {\n            if (res.code === '200') {\n              // 保存成功\n              this.$message.success('Save successful');\n              this.load(1);\n              this.formVisible = false;\n            } else {\n              this.$message.error(res.msg); // 显示错误信息\n            }\n          });\n        }\n      });\n    },\n\n    del(id) {\n      // 删除记录\n      this.$confirm('Are you sure you want to delete this record?', 'Warning', {\n        type: \"warning\"\n      }).then(() => {\n        this.$request.delete(`/submit/delete/${id}`).then(res => {\n          if (res.code === '200') {\n            // 删除成功\n            this.$message.success('Operation successful');\n            this.load(1);\n          } else {\n            this.$message.error(res.msg); // 显示错误信息\n          }\n        });\n      }).catch(() => {});\n    },\n    load(pageNum) {\n      // 加载数据\n      if (pageNum) this.pageNum = pageNum;\n      const {\n        start,\n        end,\n        status\n      } = this.searchForm;\n      const params = {\n        pageNum: this.pageNum,\n        pageSize: this.pageSize,\n        startDate: start,\n        endDate: end,\n        status: status\n      };\n      console.log('Loading data with params:', params); // 控制台打印语句\n\n      this.$request.get('/submit/selectPage', {\n        params\n      }).then(res => {\n        this.tableData = res.data?.list || [];\n        console.log(this.tableData);\n        this.total = res.data?.total || 0;\n      });\n    },\n    reset() {\n      this.searchForm.start = null;\n      this.searchForm.end = null;\n      this.searchForm.status = null;\n      this.load(1);\n    },\n    handleCurrentChange(pageNum) {\n      this.load(pageNum);\n    },\n    handleAvatarSuccess(response) {\n      this.form.img = response.data;\n    }\n  }\n};","map":{"version":3,"names":["name","data","tableData","pageNum","pageSize","total","searchForm","start","end","status","formVisible","form","user","JSON","parse","localStorage","getItem","rules","required","message","trigger","startPickerOptions","disabledDate","time","getTime","startDate","currentDate","endPickerOptions","created","load","methods","handleAdd","deal","row","$request","put","then","res","code","$message","success","error","msg","save","$refs","formRef","validate","valid","url","id","method","del","$confirm","type","delete","catch","params","endDate","console","log","get","list","reset","handleCurrentChange","handleAvatarSuccess","response","img"],"sources":["src/views/manager/Submit.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <!-- 搜索表单 -->\r\n    <el-form :inline=\"true\" :model=\"searchForm\" class=\"demo-form-inline\">\r\n      <el-form-item label=\"Date\">\r\n        <el-date-picker\r\n            v-model=\"searchForm.start\"\r\n            type=\"datetime\"\r\n            placeholder=\"Select Start Date\"\r\n            value-format=\"yyyy-MM-dd HH:mm:ss\"\r\n            :picker-options=\"startPickerOptions\"\r\n            default-time=\"00:00:00\"\r\n        />\r\n      </el-form-item>\r\n      <el-form-item>\r\n        <el-date-picker\r\n            v-model=\"searchForm.end\"\r\n            type=\"datetime\"\r\n            placeholder=\"Select End Date\"\r\n            value-format=\"yyyy-MM-dd HH:mm:ss\"\r\n            :picker-options=\"endPickerOptions\"\r\n            default-time=\"23:59:59\"\r\n        />\r\n      </el-form-item>\r\n      <el-form-item>\r\n        <el-select v-model=\"searchForm.status\" placeholder=\"Search By Status\" style=\"width: 200px; margin-left: 5px\">\r\n          <el-option label=\"Pending\" value=\"Pending\"></el-option>\r\n          <el-option label=\"Processed\" value=\"Processed\"></el-option>\r\n        </el-select>\r\n      </el-form-item>\r\n      <el-form-item>\r\n        <el-button type=\"info\" plain style=\"margin-left: 10px\" @click=\"load(1)\">Search</el-button>\r\n      </el-form-item>\r\n      <el-form-item>\r\n        <el-button type=\"warning\" plain style=\"margin-left: 10px\" @click=\"reset\">Reset</el-button>\r\n      </el-form-item>\r\n    </el-form>\r\n\r\n    <!-- 操作按钮 -->\r\n    <div class=\"operation\" v-if=\"user.role === 'USER'\">\r\n      <el-button type=\"primary\" plain @click=\"handleAdd\">Report</el-button>\r\n    </div>\r\n\r\n    <!-- 数据表格 -->\r\n    <div class=\"table\">\r\n      <el-table :data=\"tableData\" stripe>\r\n        <el-table-column prop=\"id\" label=\"ID\" width=\"80\" align=\"center\" sortable></el-table-column>\r\n        <el-table-column prop=\"img\" label=\"Report Image\" show-overflow-tooltip>\r\n          <template v-slot=\"scope\">\r\n            <div style=\"display: flex; align-items: center\">\r\n              <el-image style=\"width: 40px; height: 40px\" v-if=\"scope.row.img\"\r\n                        :src=\"scope.row.img\" :preview-src-list=\"[scope.row.img]\"></el-image>\r\n            </div>\r\n          </template>\r\n        </el-table-column>\r\n        <el-table-column prop=\"name\" label=\"Description\" show-overflow-tooltip></el-table-column>\r\n        <el-table-column prop=\"time\" label=\"Report Time\"></el-table-column>\r\n        <el-table-column prop=\"status\" label=\"Status\"></el-table-column>\r\n\r\n        <el-table-column label=\"Operations\" width=\"180\" align=\"center\" v-if=\"user.role === 'ADMIN'\">\r\n          <template v-slot=\"scope\">\r\n            <div style=\"display: flex; gap: 10px; justify-content: center; align-items: center;\">\r\n              <el-button plain type=\"primary\" :disabled=\"scope.row.status === 'Processed'\" @click=\"deal(scope.row, 'Processed')\" size=\"mini\">Processed</el-button>\r\n              <el-button plain type=\"danger\" size=\"mini\" @click=\"del(scope.row.id)\">Delete</el-button>\r\n            </div>\r\n          </template>\r\n        </el-table-column>\r\n      </el-table>\r\n\r\n      <!-- 分页 -->\r\n      <div class=\"pagination\">\r\n        <el-pagination\r\n            background\r\n            @current-change=\"handleCurrentChange\"\r\n            :current-page=\"pageNum\"\r\n            :page-sizes=\"[5, 10, 20]\"\r\n            :page-size=\"pageSize\"\r\n            layout=\"total, prev, pager, next\"\r\n            :total=\"total\">\r\n        </el-pagination>\r\n      </div>\r\n    </div>\r\n\r\n    <!-- 弹出表单 -->\r\n    <el-dialog title=\"Please report real and effective information, thank you for your cooperation!\" :visible.sync=\"formVisible\" width=\"40%\" :close-on-click-modal=\"false\" destroy-on-close>\r\n      <el-form label-width=\"100px\" style=\"padding-right: 50px\" :model=\"form\" :rules=\"rules\" ref=\"formRef\">\r\n        <el-form-item prop=\"img\" label=\"Upload Img\">\r\n          <el-upload\r\n              class=\"avatar-uploader\"\r\n              :action=\"$baseUrl + '/files/upload'\"\r\n              :headers=\"{ token: user.token }\"\r\n              list-type=\"picture\"\r\n              :on-success=\"handleAvatarSuccess\"\r\n          >\r\n            <el-button type=\"primary\">Upload Image</el-button>\r\n          </el-upload>\r\n        </el-form-item>\r\n        <el-form-item prop=\"name\" label=\"Description\">\r\n          <el-input type=\"textarea\" :rows=\"4\" v-model=\"form.name\" autocomplete=\"off\"></el-input>\r\n        </el-form-item>\r\n      </el-form>\r\n      <div slot=\"footer\" class=\"dialog-footer\">\r\n        <el-button @click=\"formVisible = false\">Cancel</el-button>\r\n        <el-button type=\"primary\" @click=\"save\">Confirm</el-button>\r\n      </div>\r\n    </el-dialog>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: \"Submit\",\r\n  data() {\r\n    return {\r\n      tableData: [],  // 表格数据\r\n      pageNum: 1,   // 当前页码\r\n      pageSize: 10,  // 每页显示条数\r\n      total: 0, // 总条数\r\n      searchForm: {\r\n        start: null,  // 开始日期\r\n        end: null,  // 结束日期\r\n        status: null, // 状态\r\n      },\r\n      formVisible: false, // 表单是否可见\r\n      form: {}, // 表单数据\r\n      user: JSON.parse(localStorage.getItem('xm-user') || '{}'), // 用户信息\r\n      rules: {\r\n        name: [\r\n          { required: true, message: 'Please enter the description', trigger: 'blur' },\r\n        ],\r\n      }\r\n    }\r\n  },\r\n  startPickerOptions: {\r\n    disabledDate(time) {\r\n      return time.getTime() < startDate.getTime() || time.getTime() > currentDate.getTime();\r\n    },\r\n  },\r\n  endPickerOptions: {\r\n    disabledDate(time) {\r\n      return time.getTime() < startDate.getTime() || time.getTime() > currentDate.getTime();\r\n    },\r\n  },\r\n  created() {\r\n    this.load(1)\r\n  },\r\n  methods: {\r\n    handleAdd() {   // 添加新数据\r\n      this.form = {\r\n        status: 'Pending'\r\n      }  // 清空表单数据\r\n      this.formVisible = true   // 打开对话框\r\n    },\r\n    deal(row, status) {\r\n      let data = { ...row, status }; // 更新数据的状态\r\n      this.$request.put('/submit/update', data).then(res => {\r\n        if (res.code === '200') {\r\n          this.$message.success('Operation successful');\r\n          this.load(1);\r\n        } else {\r\n          this.$message.error(res.msg);\r\n        }\r\n      });\r\n    },\r\n    save() {   // 保存表单数据\r\n      this.$refs.formRef.validate((valid) => {\r\n        if (valid) {\r\n          this.$request({\r\n            url: this.form.id ? '/submit/update' : '/submit/add',\r\n            method: this.form.id ? 'PUT' : 'POST',\r\n            data: this.form\r\n          }).then(res => {\r\n            if (res.code === '200') {  // 保存成功\r\n              this.$message.success('Save successful');\r\n              this.load(1);\r\n              this.formVisible = false;\r\n            } else {\r\n              this.$message.error(res.msg);  // 显示错误信息\r\n            }\r\n          });\r\n        }\r\n      });\r\n    },\r\n    del(id) {   // 删除记录\r\n      this.$confirm('Are you sure you want to delete this record?', 'Warning', { type: \"warning\" }).then(() => {\r\n        this.$request.delete(`/submit/delete/${id}`).then(res => {\r\n          if (res.code === '200') {   // 删除成功\r\n            this.$message.success('Operation successful');\r\n            this.load(1);\r\n          } else {\r\n            this.$message.error(res.msg);  // 显示错误信息\r\n          }\r\n        });\r\n      }).catch(() => {});\r\n    },\r\n    load(pageNum) {  // 加载数据\r\n      if (pageNum) this.pageNum = pageNum;\r\n      const { start, end, status } = this.searchForm;\r\n      const params = {\r\n        pageNum: this.pageNum,\r\n        pageSize: this.pageSize,\r\n        startDate: start,\r\n        endDate: end,\r\n        status: status,\r\n      };\r\n\r\n      console.log('Loading data with params:', params); // 控制台打印语句\r\n\r\n      this.$request.get('/submit/selectPage', { params }).then(res => {\r\n        this.tableData = res.data?.list || [];\r\n        console.log(this.tableData);\r\n        this.total = res.data?.total || 0;\r\n      });\r\n\r\n    },\r\n    reset() {\r\n      this.searchForm.start = null;\r\n      this.searchForm.end = null;\r\n      this.searchForm.status = null;\r\n      this.load(1);\r\n    },\r\n    handleCurrentChange(pageNum) {\r\n      this.load(pageNum);\r\n    },\r\n    handleAvatarSuccess(response) {\r\n      this.form.img = response.data;\r\n    },\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.demo-form-inline .el-form-item {\r\n  margin-right: 10px;\r\n}\r\n</style>\r\n"],"mappings":"AA8GA;EACAA,IAAA;EACAC,KAAA;IACA;MACAC,SAAA;MAAA;MACAC,OAAA;MAAA;MACAC,QAAA;MAAA;MACAC,KAAA;MAAA;MACAC,UAAA;QACAC,KAAA;QAAA;QACAC,GAAA;QAAA;QACAC,MAAA;MACA;;MACAC,WAAA;MAAA;MACAC,IAAA;MAAA;MACAC,IAAA,EAAAC,IAAA,CAAAC,KAAA,CAAAC,YAAA,CAAAC,OAAA;MAAA;MACAC,KAAA;QACAjB,IAAA,GACA;UAAAkB,QAAA;UAAAC,OAAA;UAAAC,OAAA;QAAA;MAEA;IACA;EACA;EACAC,kBAAA;IACAC,aAAAC,IAAA;MACA,OAAAA,IAAA,CAAAC,OAAA,KAAAC,SAAA,CAAAD,OAAA,MAAAD,IAAA,CAAAC,OAAA,KAAAE,WAAA,CAAAF,OAAA;IACA;EACA;EACAG,gBAAA;IACAL,aAAAC,IAAA;MACA,OAAAA,IAAA,CAAAC,OAAA,KAAAC,SAAA,CAAAD,OAAA,MAAAD,IAAA,CAAAC,OAAA,KAAAE,WAAA,CAAAF,OAAA;IACA;EACA;EACAI,QAAA;IACA,KAAAC,IAAA;EACA;EACAC,OAAA;IACAC,UAAA;MAAA;MACA,KAAApB,IAAA;QACAF,MAAA;MACA;MACA,KAAAC,