{"ast":null,"code":"export default {\n  name: \"Adopt\",\n  data() {\n    return {\n      tableData: [],\n      pageNum: 1,\n      pageSize: 10,\n      total: 0,\n      animalId: null,\n      userId: null,\n      // 用于选择用户的ID\n      status: null,\n      formVisible: false,\n      // 控制表单对话框的显示\n      form: {},\n      // 表单数据\n      user: JSON.parse(localStorage.getItem('xm-user') || '{}'),\n      ids: [],\n      animalData: [],\n      userData: [],\n      // 用户数据列表\n      availableAnimals: [],\n      // 可用动物列表\n      searchQuery: '',\n      userQuery: '',\n      // 用户搜索输入框的绑定值\n      rules: {\n        animalId: [{\n          required: true,\n          message: \"Please select an animal\",\n          trigger: \"change\"\n        }],\n        userId: [{\n          required: true,\n          message: \"Please select a user\",\n          trigger: \"change\"\n        }],\n        time: [{\n          required: true,\n          message: \"Please select adopt time\",\n          trigger: \"change\"\n        }],\n        status: [{\n          required: true,\n          message: \"Please select status\",\n          trigger: \"change\"\n        }]\n      }\n    };\n  },\n  created() {\n    this.load(1);\n    this.loadAnimal();\n    this.loadUsers(); // 调用加载用户数据的方法\n  },\n\n  computed: {\n    filteredAnimalData() {\n      if (!this.searchQuery) {\n        return [];\n      }\n      return this.animalData.filter(item => item.name.toLowerCase().includes(this.searchQuery.toLowerCase()));\n    },\n    filteredUserData() {\n      // 改名为 filteredUserData\n      if (!this.userQuery) {\n        return [];\n      }\n      return this.userData.filter(item => item.name.toLowerCase().includes(this.userQuery.toLowerCase()));\n    }\n  },\n  methods: {\n    handleAdd() {\n      // 添加新的领养记录\n      this.form = {}; // 清空表单数据\n      this.formVisible = true; // 打开对话框\n    },\n\n    loadAnimal() {\n      this.$request.get('/adopt/selectAnimal').then(res => {\n        if (res.code === '200') {\n          this.animalData = res.data;\n          this.availableAnimals = res.data.filter(animal => animal.status === 'Available'); // 仅加载状态为Available的动物\n        } else {\n          this.$message.error(res.msg);\n        }\n      }).catch(error => {\n        console.error('Request failed', error);\n        this.$message.error('Failed to load animal data.');\n      });\n    },\n    loadUsers() {\n      // 新增的方法来加载用户数据\n      this.$request.get('/adopt/selectUser').then(res => {\n        if (res.code === '200') {\n          this.userData = res.data; // 将响应数据存储到 userData\n          console.log(this.userData); // 打印 userData 以确认数据正确加载\n        } else {\n          this.$message.error(res.msg);\n        }\n      }).catch(error => {\n        console.error('Request failed', error);\n        this.$message.error('Failed to load user data.');\n      });\n    },\n    save() {\n      this.$refs.formRef.validate(valid => {\n        if (valid) {\n          this.$request({\n            url: this.form.id ? '/adopt/update' : '/adopt/add',\n            method: this.form.id ? 'PUT' : 'POST',\n            data: this.form\n          }).then(res => {\n            if (res.code === '200') {\n              this.$message.success('Saved successfully');\n              this.load(1); // 重新加载数据\n              this.formVisible = false; // 关闭对话框\n            } else {\n              this.$message.error(res.msg);\n            }\n          });\n        }\n      });\n    },\n    GiveUp(row) {\n      this.$confirm('Are you sure you want to give up the adoption? Giving up the adoption means you will no longer be the potential adopter for this animal. If you have any concerns or need assistance, please contact our support team.', 'Confirm', {\n        type: \"warning\"\n      }).then(response => {\n        this.form = JSON.parse(JSON.stringify(row));\n        this.form.status = 'GiveUp Adoption';\n        this.save();\n      }).catch(() => {});\n    },\n    handleSelectionChange(rows) {\n      this.ids = rows.map(v => v.id);\n    },\n    delBatch() {\n      if (!this.ids.length) {\n        this.$message.warning('Please select data');\n        return;\n      }\n      this.$confirm('Are you sure you want to delete these data?', 'Confirm Delete', {\n        type: \"warning\"\n      }).then(response => {\n        this.$request.delete('/adopt/delete/batch', {\n          data: this.ids\n        }).then(res => {\n          if (res.code === '200') {\n            this.$message.success('Operation successful');\n            this.load(1);\n          } else {\n            this.$message.error(res.msg);\n          }\n        });\n      }).catch(() => {});\n    },\n    load(pageNum) {\n      if (pageNum) this.pageNum = pageNum;\n      this.$request.get('/adopt/selectPage', {\n        params: {\n          pageNum: this.pageNum,\n          pageSize: this.pageSize,\n          status: this.status,\n          animalId: this.animalId,\n          userId: this.userId // 改为 userId\n        }\n      }).then(res => {\n        this.tableData = res.data?.list;\n        this.total = res.data?.total;\n      });\n    },\n    reset() {\n      this.status = null;\n      this.animalId = null;\n      this.userId = null; // 重置 userId\n      this.searchQuery = '';\n      this.userQuery = ''; // 重置 userQuery\n      this.load(1);\n    },\n    handleCurrentChange(pageNum) {\n      this.load(pageNum);\n    },\n    handleSearch() {\n      // 在这里可以处理额外的搜索逻辑，例如记录搜索关键词或其他操作\n      // 当前示例中没有具体操作，因为过滤逻辑在 computed 属性中\n    },\n    handleUserSearch() {// 改名为 handleUserSearch\n      // 这里处理额外的用户搜索逻辑\n    },\n    selectAnimal(id) {\n      this.animalId = id;\n      this.searchQuery = this.animalData.find(item => item.id === id).name;\n    },\n    selectUser(id) {\n      // 新增 selectUser 方法\n      this.userId = id;\n      this.userQuery = this.userData.find(item => item.id === id).name;\n    }\n  }\n};","map":{"version":3,"names":["name","data","tableData","pageNum","pageSize","total","animalId","userId","status","formVisible","form","user","JSON","parse","localStorage","getItem","ids","animalData","userData","availableAnimals","searchQuery","userQuery","rules","required","message","trigger","time","created","load","loadAnimal","loadUsers","computed","filteredAnimalData","filter","item","toLowerCase","includes","filteredUserData","methods","handleAdd","$request","get","then","res","code","animal","$message","error","msg","catch","console","log","save","$refs","formRef","validate","valid","url","id","method","success","GiveUp","row","$confirm","type","response","stringify","handleSelectionChange","rows","map","v","delBatch","length","warning","delete","params","list","reset","handleCurrentChange","handleSearch","handleUserSearch","selectAnimal","find","selectUser"],"sources":["src/views/manager/Adopt.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <div class=\"search\">\r\n      <!-- 搜索框 -->\r\n      <el-input\r\n          v-model=\"searchQuery\"\r\n          placeholder=\"Search By Animal Name\"\r\n          style=\"width: 200px\"\r\n          @input=\"handleSearch\"\r\n      ></el-input>\r\n      <ul v-if=\"filteredAnimalData.length > 0\" class=\"search-results\">\r\n        <li\r\n            v-for=\"item in filteredAnimalData\"\r\n            :key=\"item.id\"\r\n            @click=\"selectAnimal(item.id)\"\r\n        >\r\n          {{ item.name }}\r\n        </li>\r\n      </ul>\r\n\r\n      <!-- 收养人搜索框 -->\r\n      <el-input\r\n          v-model=\"adopterQuery\"\r\n          placeholder=\"Search By Adopter\"\r\n          style=\"width: 200px; margin-left: 5px\"\r\n          @input=\"handleAdopterSearch\"\r\n          v-if=\"user.role === 'ADMIN'\"\r\n      ></el-input>\r\n      <ul v-if=\"filteredAdopterData.length > 0\" class=\"search-results\">\r\n        <li\r\n            v-for=\"item in filteredAdopterData\"\r\n            :key=\"item.id\"\r\n            @click=\"selectAdopter(item.id)\"\r\n        >\r\n          {{ item.name }}\r\n        </li>\r\n      </ul>\r\n\r\n      <!-- 状态选择框 -->\r\n      <el-select\r\n          v-model=\"status\"\r\n          placeholder=\"Search By Status\"\r\n          style=\"width: 200px; margin-left: 5px\"\r\n      >\r\n        <el-option label=\"Adopted\" value=\"Adopted\"></el-option>\r\n        <el-option label=\"In Process\" value=\"In Process\"></el-option>\r\n      </el-select>\r\n\r\n      <el-button\r\n          type=\"info\"\r\n          plain\r\n          style=\"margin-left: 10px\"\r\n          @click=\"load(1)\"\r\n      >Search</el-button\r\n      >\r\n      <el-button\r\n          type=\"warning\"\r\n          plain\r\n          style=\"margin-left: 10px\"\r\n          @click=\"reset\"\r\n      >Reset</el-button\r\n      >\r\n    </div>\r\n\r\n    <div class=\"operation\" v-if=\"user.role !== 'USER'\">\r\n      <el-button type=\"primary\" plain @click=\"handleAdd\"\r\n      >Add New Adopt Record</el-button\r\n      >\r\n      <el-button type=\"danger\" plain @click=\"delBatch\">Batch Delete</el-button>\r\n    </div>\r\n\r\n    <div class=\"table\">\r\n      <el-table :data=\"tableData\" stripe @selection-change=\"handleSelectionChange\">\r\n        <!-- Selection Column -->\r\n        <el-table-column type=\"selection\" width=\"55\" align=\"center\"></el-table-column>\r\n\r\n        <!-- ID Column -->\r\n        <el-table-column\r\n            prop=\"id\"\r\n            label=\"ID\"\r\n            width=\"80\"\r\n            align=\"center\"\r\n            sortable\r\n        ></el-table-column>\r\n\r\n        <!-- Image Column -->\r\n        <el-table-column prop=\"animalImg\" label=\"Image\" show-overflow-tooltip>\r\n          <template v-slot=\"scope\">\r\n            <div style=\"display: flex; align-items: center\">\r\n              <el-image\r\n                  style=\"width: 40px; height: 40px; border-radius: 50%\"\r\n                  v-if=\"scope.row.animalImg\"\r\n                  :src=\"scope.row.animalImg\"\r\n                  :preview-src-list=\"[scope.row.animalImg]\"\r\n              ></el-image>\r\n            </div>\r\n          </template>\r\n        </el-table-column>\r\n\r\n        <!-- Name Column -->\r\n        <el-table-column\r\n            prop=\"animalName\"\r\n            label=\"Name\"\r\n            show-overflow-tooltip\r\n        ></el-table-column>\r\n\r\n        <!-- Adopter Column -->\r\n        <el-table-column\r\n            prop=\"userName\"\r\n            label=\"Adopter\"\r\n            show-overflow-tooltip\r\n        ></el-table-column>\r\n\r\n        <!-- Adopt Time Column -->\r\n        <el-table-column prop=\"time\" label=\"Adopt Time\"></el-table-column>\r\n\r\n        <!-- Adoption Status Column -->\r\n        <el-table-column prop=\"status\" label=\"Adoption Status\"></el-table-column>\r\n\r\n        <!-- Actions Column: Only visible to non-ADMIN users -->\r\n        <el-table-column\r\n            v-if=\"user.role !== 'ADMIN'\"\r\n            label=\"Actions\"\r\n            width=\"180\"\r\n            align=\"center\"\r\n        >\r\n          <template v-slot=\"scope\">\r\n            <el-button\r\n                plain\r\n                type=\"danger\"\r\n                size=\"mini\"\r\n                :disabled=\"scope.row.status === 'GiveUp Adoption'\"\r\n                @click=\"GiveUp(scope.row)\"\r\n            >\r\n              Give Up Adoption\r\n            </el-button>\r\n          </template>\r\n        </el-table-column>\r\n      </el-table>\r\n    </div>\r\n\r\n    <div class=\"pagination\">\r\n      <el-pagination\r\n          background\r\n          @current-change=\"handleCurrentChange\"\r\n          :current-page=\"pageNum\"\r\n          :page-sizes=\"[5, 10, 20]\"\r\n          :page-size=\"pageSize\"\r\n          layout=\"total, prev, pager, next\"\r\n          :total=\"total\"\r\n      ></el-pagination>\r\n    </div>\r\n\r\n    <!-- Add/Edit Adoption Record Dialog -->\r\n    <el-dialog\r\n        title=\"Adoption Record\"\r\n        :visible.sync=\"formVisible\"\r\n        width=\"40%\"\r\n        :close-on-click-modal=\"false\"\r\n        destroy-on-close\r\n    >\r\n      <el-form\r\n          label-width=\"120px\"\r\n          style=\"padding-right: 50px\"\r\n          :model=\"form\"\r\n          :rules=\"rules\"\r\n          ref=\"formRef\"\r\n      >\r\n        <el-form-item prop=\"animalId\" label=\"Animal\">\r\n          <el-select\r\n              v-model=\"form.animalId\"\r\n              placeholder=\"Select Animal\"\r\n              style=\"width: 100%\"\r\n          >\r\n            <el-option\r\n                v-for=\"animal in availableAnimals\"\r\n                :key=\"animal.id\"\r\n                :label=\"animal.name\"\r\n                :value=\"animal.id\"\r\n            ></el-option>\r\n          </el-select>\r\n        </el-form-item>\r\n\r\n        <!-- User Selector -->\r\n        <el-form-item prop=\"userId\" label=\"User\">\r\n          <el-select\r\n              v-model=\"form.userId\"\r\n              placeholder=\"Select User\"\r\n              style=\"width: 100%\"\r\n          >\r\n            <el-option\r\n                v-for=\"user in userData\"\r\n                :key=\"user.id\"\r\n                :label=\"user.name\"\r\n                :value=\"user.id\"\r\n            ></el-option>\r\n          </el-select>\r\n        </el-form-item>\r\n\r\n        <el-form-item prop=\"time\" label=\"Adopt Time\">\r\n          <el-date-picker\r\n              v-model=\"form.time\"\r\n              type=\"datetime\"\r\n              placeholder=\"Select Date and Time\"\r\n              style=\"width: 100%\"\r\n          ></el-date-picker>\r\n        </el-form-item>\r\n        <el-form-item prop=\"status\" label=\"Status\">\r\n          <el-select\r\n              v-model=\"form.status\"\r\n              placeholder=\"Select Status\"\r\n              style=\"width: 100%\"\r\n          >\r\n            <el-option label=\"In Process\" value=\"In Process\"></el-option>\r\n            <el-option label=\"Adopted\" value=\"Adopted\"></el-option>\r\n          </el-select>\r\n        </el-form-item>\r\n      </el-form>\r\n      <div slot=\"footer\" class=\"dialog-footer\">\r\n        <el-button @click=\"formVisible = false\">Cancel</el-button>\r\n        <el-button type=\"primary\" @click=\"save\">Confirm</el-button>\r\n      </div>\r\n    </el-dialog>\r\n  </div>\r\n</template>\r\n\r\n\r\n<script>\r\nexport default {\r\n  name: \"Adopt\",\r\n  data() {\r\n    return {\r\n      tableData: [],\r\n      pageNum: 1,\r\n      pageSize: 10,\r\n      total: 0,\r\n      animalId: null,\r\n      userId: null, // 用于选择用户的ID\r\n      status: null,\r\n      formVisible: false, // 控制表单对话框的显示\r\n      form: {}, // 表单数据\r\n      user: JSON.parse(localStorage.getItem('xm-user') || '{}'),\r\n      ids: [],\r\n      animalData: [],\r\n      userData: [], // 用户数据列表\r\n      availableAnimals: [], // 可用动物列表\r\n      searchQuery: '',\r\n      userQuery: '', // 用户搜索输入框的绑定值\r\n      rules: {\r\n        animalId: [\r\n          { required: true, message: \"Please select an animal\", trigger: \"change\" },\r\n        ],\r\n        userId: [\r\n          { required: true, message: \"Please select a user\", trigger: \"change\" },\r\n        ],\r\n        time: [\r\n          { required: true, message: \"Please select adopt time\", trigger: \"change\" },\r\n        ],\r\n        status: [\r\n          { required: true, message: \"Please select status\", trigger: \"change\" },\r\n        ],\r\n      },\r\n    };\r\n  },\r\n  created() {\r\n    this.load(1);\r\n    this.loadAnimal();\r\n    this.loadUsers();  // 调用加载用户数据的方法\r\n  },\r\n  computed: {\r\n    filteredAnimalData() {\r\n      if (!this.searchQuery) {\r\n        return [];\r\n      }\r\n      return this.animalData.filter(item =>\r\n          item.name.toLowerCase().includes(this.searchQuery.toLowerCase())\r\n      );\r\n    },\r\n    filteredUserData() { // 改名为 filteredUserData\r\n      if (!this.userQuery) {\r\n        return [];\r\n      }\r\n      return this.userData.filter(item =>\r\n          item.name.toLowerCase().includes(this.userQuery.toLowerCase())\r\n      );\r\n    }\r\n  },\r\n  methods: {\r\n    handleAdd() {   // 添加新的领养记录\r\n      this.form = {};  // 清空表单数据\r\n      this.formVisible = true;   // 打开对话框\r\n    },\r\n    loadAnimal() {\r\n      this.$request.get('/adopt/selectAnimal').then(res => {\r\n        if (res.code === '200') {\r\n          this.animalData = res.data;\r\n          this.availableAnimals = res.data.filter(animal => animal.status === 'Available'); // 仅加载状态为Available的动物\r\n        } else {\r\n          this.$message.error(res.msg);\r\n        }\r\n      }).catch(error => {\r\n        console.error('Request failed', error);\r\n        this.$message.error('Failed to load animal data.');\r\n      });\r\n    },\r\n    loadUsers() { // 新增的方法来加载用户数据\r\n      this.$request.get('/adopt/selectUser').then(res => {\r\n        if (res.code === '200') {\r\n          this.userData = res.data; // 将响应数据存储到 userData\r\n          console.log(this.userData); // 打印 userData 以确认数据正确加载\r\n        } else {\r\n          this.$message.error(res.msg);\r\n        }\r\n      }).catch(error => {\r\n        console.error('Request failed', error);\r\n        this.$message.error('Failed to load user data.');\r\n      });\r\n    },\r\n    save() {\r\n      this.$refs.formRef.validate((valid) => {\r\n        if (valid) {\r\n          this.$request({\r\n            url: this.form.id ? '/adopt/update' : '/adopt/add',\r\n            method: this.form.id ? 'PUT' : 'POST',\r\n            data: this.form\r\n          }).then(res => {\r\n            if (res.code === '200') {\r\n              this.$message.success('Saved successfully');\r\n              this.load(1); // 重新加载数据\r\n              this.formVisible = false; // 关闭对话框\r\n            } else {\r\n              this.$message.error(res.msg);\r\n            }\r\n          });\r\n        }\r\n      });\r\n    },\r\n    GiveUp(row) {\r\n      this.$confirm('Are you sure you want to give up the adoption? Giving up the adoption means you will no longer be the potential adopter for this animal. If you have any concerns or need assistance, please contact our support team.', 'Confirm', {type: \"warning\"}).then(response => {\r\n        this.form = JSON.parse(JSON.stringify(row));\r\n        this.form.status = 'GiveUp Adoption';\r\n        this.save();\r\n      }).catch(() => {\r\n      });\r\n    },\r\n    handleSelectionChange(rows) {\r\n      this.ids = rows.map(v => v.id);\r\n    },\r\n    delBatch() {\r\n      if (!this.ids.length) {\r\n        this.$message.warning('Please select data');\r\n        return;\r\n      }\r\n      this.$confirm('Are you sure you want to delete these data?', 'Confirm Delete', {type: \"warning\"}).then(response => {\r\n        this.$request.delete('/adopt/delete/batch', {data: this.ids}).then(res => {\r\n          if (res.code === '200') {\r\n            this.$message.success('Operation successful');\r\n            this.load(1);\r\n          } else {\r\n            this.$message.error(res.msg);\r\n          }\r\n        });\r\n      }).catch(() => {\r\n      });\r\n    },\r\n    load(pageNum) {\r\n      if (pageNum) this.pageNum = pageNum;\r\n      this.$request.get('/adopt/selectPage', {\r\n        params: {\r\n          pageNum: this.pageNum,\r\n          pageSize: this.pageSize,\r\n          status: this.status,\r\n          animalId: this.animalId,\r\n          userId: this.userId // 改为 userId\r\n        }\r\n      }).then(res => {\r\n        this.tableData = res.data?.list;\r\n        this.total = res.data?.total;\r\n      });\r\n    },\r\n    reset() {\r\n      this.status = null;\r\n      this.animalId = null;\r\n      this.userId = null; // 重置 userId\r\n      this.searchQuery = '';\r\n      this.userQuery = ''; // 重置 userQuery\r\n      this.load(1);\r\n    },\r\n    handleCurrentChange(pageNum) {\r\n      this.load(pageNum);\r\n    },\r\n    handleSearch() {\r\n      // 在这里可以处理额外的搜索逻辑，例如记录搜索关键词或其他操作\r\n      // 当前示例中没有具体操作，因为过滤逻辑在 computed 属性中\r\n    },\r\n    handleUserSearch() { // 改名为 handleUserSearch\r\n      // 这里处理额外的用户搜索逻辑\r\n    },\r\n    selectAnimal(id) {\r\n      this.animalId = id;\r\n      this.searchQuery = this.animalData.find(item => item.id === id).name;\r\n    },\r\n    selectUser(id) { // 新增 selectUser 方法\r\n      this.userId = id;\r\n      this.userQuery = this.userData.find(item => item.id === id).name;\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n\r\n<style scoped>\r\n.search-results {\r\n  list-style-type: none;\r\n  padding: 0;\r\n  margin: 0;\r\n  border: 1px solid #dcdcdc;\r\n  max-height: 150px;\r\n  overflow-y: auto;\r\n  background: white;\r\n}\r\n\r\n.search-results li {\r\n  padding: 8px;\r\n  cursor: pointer;\r\n}\r\n\r\n.search-results li:hover {\r\n  background: #f0f0f0;\r\n}\r\n</style>\r\n"],"mappings":"AAoOA;EACAA,IAAA;EACAC,KAAA;IACA;MACAC,SAAA;MACAC,OAAA;MACAC,QAAA;MACAC,KAAA;MACAC,QAAA;MACAC,MAAA;MAAA;MACAC,MAAA;MACAC,WAAA;MAAA;MACAC,IAAA;MAAA;MACAC,IAAA,EAAAC,IAAA,CAAAC,KAAA,CAAAC,YAAA,CAAAC,OAAA;MACAC,GAAA;MACAC,UAAA;MACAC,QAAA;MAAA;MACAC,gBAAA;MAAA;MACAC,WAAA;MACAC,SAAA;MAAA;MACAC,KAAA;QACAhB,QAAA,GACA;UAAAiB,QAAA;UAAAC,OAAA;UAAAC,OAAA;QAAA,EACA;QACAlB,MAAA,GACA;UAAAgB,QAAA;UAAAC,OAAA;UAAAC,OAAA;QAAA,EACA;QACAC,IAAA,GACA;UAAAH,QAAA;UAAAC,OAAA;UAAAC,OAAA;QAAA,EACA;QACAjB,MAAA,GACA;UAAAe,QAAA;UAAAC,OAAA;UAAAC,OAAA;QAAA;MAEA;IACA;EACA;EACAE,QAAA;IACA,KAAAC,IAAA;IACA,KAAAC,UAAA;IACA,KAAAC,SAAA;EACA;;EACAC,QAAA;IACAC,mBAAA;MACA,UAAAZ,WAAA;QACA;MACA;MACA,YAAAH,UAAA,CAAAgB,MAAA,CAAAC,IAAA,IACAA,IAAA,CAAAlC,IAAA,CAAAmC,WAAA,GAAAC,QAAA,MAAAhB,WAAA,CAAAe,WAAA,GACA;IACA;IACAE,iBAAA;MAAA;MACA,UAAAhB,SAAA;QACA;MACA;MACA,YAAAH,QAAA,CAAAe,MAAA,CAAAC,IAAA,IACAA,IAAA,CAAAlC,IAAA,CAAAmC,WAAA,GAAAC,QAAA,MAAAf,SAAA,CAAAc,WAAA,GACA;IACA;EACA;EACAG,OAAA;IACAC,UAAA;MAAA;MACA,KAAA7B,IAAA;MACA,KAAAD,WAAA;IACA;;IACAoB,WAAA;MACA,KAAAW,QAAA,CAAAC,GAAA,wBAAAC,IAAA,CAAAC,GAAA;QACA,IAAAA,GAAA,CAAAC,IAAA;UACA,KAAA3B,UAAA,GAAA0B,GAAA,CAAA1C,IAA