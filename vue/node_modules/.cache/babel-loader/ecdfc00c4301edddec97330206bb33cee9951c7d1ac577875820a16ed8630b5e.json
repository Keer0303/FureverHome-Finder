{"ast":null,"code":"export default {\n  name: \"Application\",\n  data() {\n    return {\n      applications: [],\n      pageNum: 1,\n      pageSize: 10,\n      total: 0,\n      search: {\n        name: '',\n        status: ''\n      },\n      formVisible: false,\n      form: {\n        animalId: '',\n        reason: ''\n      },\n      user: JSON.parse(localStorage.getItem('xm-user') || '{}'),\n      rules: {\n        animalId: [{\n          required: true,\n          message: 'Please enter the animal ID',\n          trigger: 'blur'\n        }],\n        reason: [{\n          required: true,\n          message: 'Please enter the reason for adoption',\n          trigger: 'blur'\n        }]\n      }\n    };\n  },\n  created() {\n    this.loadApplications();\n  },\n  methods: {\n    loadApplications() {\n      this.$request.get('/application/selectPage', {\n        params: {\n          pageNum: this.pageNum,\n          pageSize: this.pageSize,\n          name: this.search.name,\n          status: this.search.status\n        }\n      }).then(res => {\n        if (res.data) {\n          this.applications = res.data.list;\n          this.total = res.data.total;\n        } else {\n          this.$message.error('Failed to load applications');\n        }\n      }).catch(error => {\n        console.error('Error loading applications:', error);\n        this.$message.error('An error occurred while loading applications');\n      });\n    },\n    resetSearch() {\n      this.search.name = '';\n      this.search.status = '';\n      this.loadApplications();\n    },\n    showAddForm() {\n      this.form = {\n        animalId: '',\n        reason: ''\n      };\n      this.formVisible = true;\n    },\n    submitApplication() {\n      this.$refs.formRef.validate(valid => {\n        if (valid) {\n          this.$request.post('/application/add', this.form).then(res => {\n            if (res.data) {\n              this.$message.success('Application submitted successfully');\n              this.formVisible = false;\n              this.loadApplications();\n            } else {\n              this.$message.error('Failed to submit application');\n            }\n          }).catch(error => {\n            console.error('Error submitting application:', error);\n            this.$message.error('An error occurred while submitting the application');\n          });\n        }\n      });\n    },\n    updateStatus(application, status) {\n      const updatedApplication = {\n        ...application,\n        status\n      };\n      this.$request.put('/application/update', updatedApplication).then(res => {\n        if (res.data) {\n          this.$message.success('Application status updated successfully');\n          this.loadApplications();\n        } else {\n          this.$message.error('Failed to update application status');\n        }\n      }).catch(error => {\n        console.error('Error updating application status:', error);\n        this.$message.error('An error occurred while updating the application status');\n      });\n    },\n    handlePageChange(pageNum) {\n      this.pageNum = pageNum;\n      this.loadApplications();\n    }\n  }\n};","map":{"version":3,"names":["name","data","applications","pageNum","pageSize","total","search","status","formVisible","form","animalId","reason","user","JSON","parse","localStorage","getItem","rules","required","message","trigger","created","loadApplications","methods","$request","get","params","then","res","list","$message","error","catch","console","resetSearch","showAddForm","submitApplication","$refs","formRef","validate","valid","post","success","updateStatus","application","updatedApplication","put","handlePageChange"],"sources":["src/views/manager/Applications.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <!-- 搜索栏 -->\r\n    <div class=\"search\">\r\n      <el-input placeholder=\"Search By Description\" style=\"width: 200px\" v-model=\"search.name\"></el-input>\r\n      <el-select v-model=\"search.status\" placeholder=\"Search By Status\" style=\"width: 200px; margin-left: 5px\">\r\n        <el-option label=\"Pending\" value=\"Pending\"></el-option>\r\n        <el-option label=\"Approved\" value=\"Approved\"></el-option>\r\n        <el-option label=\"Rejected\" value=\"Rejected\"></el-option>\r\n      </el-select>\r\n      <el-button type=\"info\" plain style=\"margin-left: 10px\" @click=\"loadApplications\">Search</el-button>\r\n      <el-button type=\"warning\" plain style=\"margin-left: 10px\" @click=\"resetSearch\">Reset</el-button>\r\n    </div>\r\n\r\n    <!-- 操作栏 -->\r\n    <div class=\"operation\" v-if=\"user.role === 'USER'\">\r\n      <el-button type=\"primary\" plain @click=\"showAddForm\">Apply</el-button>\r\n    </div>\r\n\r\n    <!-- 申请列表 -->\r\n    <el-table :data=\"applications\" stripe>\r\n      <el-table-column prop=\"id\" label=\"ID\" width=\"80\" align=\"center\" sortable></el-table-column>\r\n      <el-table-column prop=\"name\" label=\"Name\" show-overflow-tooltip></el-table-column>\r\n      <el-table-column prop=\"email\" label=\"Email\" show-overflow-tooltip></el-table-column>\r\n      <el-table-column prop=\"phone\" label=\"Phone\"></el-table-column>\r\n      <el-table-column prop=\"reason\" label=\"Reason\"></el-table-column>\r\n      <el-table-column prop=\"status\" label=\"Status\"></el-table-column>\r\n      <el-table-column label=\"Operations\" width=\"180\" align=\"center\" v-if=\"user.role === 'ADMIN'\">\r\n        <template v-slot=\"scope\">\r\n          <div style=\"display: flex; gap: 4px;\">\r\n            <el-button plain type=\"primary\" @click=\"updateStatus(scope.row, 'Approved')\" size=\"mini\">Approve</el-button>\r\n            <el-button plain type=\"danger\" @click=\"updateStatus(scope.row, 'Rejected')\" size=\"mini\">Reject</el-button>\r\n          </div>\r\n        </template>\r\n      </el-table-column>\r\n    </el-table>\r\n\r\n    <!-- 分页 -->\r\n    <div class=\"pagination\">\r\n      <el-pagination\r\n          background\r\n          @current-change=\"handlePageChange\"\r\n          :current-page=\"pageNum\"\r\n          :page-size=\"pageSize\"\r\n          layout=\"total, prev, pager, next\"\r\n          :total=\"total\">\r\n      </el-pagination>\r\n    </div>\r\n\r\n    <!-- 添加申请表单 -->\r\n    <el-dialog title=\"Apply for Adoption\" :visible.sync=\"formVisible\" width=\"40%\" :close-on-click-modal=\"false\" destroy-on-close>\r\n      <el-form label-width=\"100px\" style=\"padding-right: 50px\" :model=\"form\" :rules=\"rules\" ref=\"formRef\">\r\n        <el-form-item prop=\"animalId\" label=\"Animal ID\">\r\n          <el-input v-model=\"form.animalId\" autocomplete=\"off\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item prop=\"reason\" label=\"Reason\">\r\n          <el-input type=\"textarea\" :rows=\"4\" v-model=\"form.reason\" autocomplete=\"off\"></el-input>\r\n        </el-form-item>\r\n      </el-form>\r\n      <div slot=\"footer\" class=\"dialog-footer\">\r\n        <el-button @click=\"formVisible = false\">Cancel</el-button>\r\n        <el-button type=\"primary\" @click=\"submitApplication\">Submit</el-button>\r\n      </div>\r\n    </el-dialog>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: \"Application\",\r\n  data() {\r\n    return {\r\n      applications: [],\r\n      pageNum: 1,\r\n      pageSize: 10,\r\n      total: 0,\r\n      search: {\r\n        name: '',\r\n        status: ''\r\n      },\r\n      formVisible: false,\r\n      form: {\r\n        animalId: '',\r\n        reason: ''\r\n      },\r\n      user: JSON.parse(localStorage.getItem('xm-user') || '{}'),\r\n      rules: {\r\n        animalId: [\r\n          { required: true, message: 'Please enter the animal ID', trigger: 'blur' }\r\n        ],\r\n        reason: [\r\n          { required: true, message: 'Please enter the reason for adoption', trigger: 'blur' }\r\n        ]\r\n      }\r\n    }\r\n  },\r\n  created() {\r\n    this.loadApplications();\r\n  },\r\n  methods: {\r\n    loadApplications() {\r\n      this.$request.get('/application/selectPage', {\r\n        params: {\r\n          pageNum: this.pageNum,\r\n          pageSize: this.pageSize,\r\n          name: this.search.name,\r\n          status: this.search.status\r\n        }\r\n      }).then(res => {\r\n        if (res.data) {\r\n          this.applications = res.data.list;\r\n          this.total = res.data.total;\r\n        } else {\r\n          this.$message.error('Failed to load applications');\r\n        }\r\n      }).catch(error => {\r\n        console.error('Error loading applications:', error);\r\n        this.$message.error('An error occurred while loading applications');\r\n      });\r\n    },\r\n    resetSearch() {\r\n      this.search.name = '';\r\n      this.search.status = '';\r\n      this.loadApplications();\r\n    },\r\n    showAddForm() {\r\n      this.form = {\r\n        animalId: '',\r\n        reason: ''\r\n      };\r\n      this.formVisible = true;\r\n    },\r\n    submitApplication() {\r\n      this.$refs.formRef.validate(valid => {\r\n        if (valid) {\r\n          this.$request.post('/application/add', this.form).then(res => {\r\n            if (res.data) {\r\n              this.$message.success('Application submitted successfully');\r\n              this.formVisible = false;\r\n              this.loadApplications();\r\n            } else {\r\n              this.$message.error('Failed to submit application');\r\n            }\r\n          }).catch(error => {\r\n            console.error('Error submitting application:', error);\r\n            this.$message.error('An error occurred while submitting the application');\r\n          });\r\n        }\r\n      });\r\n    },\r\n    updateStatus(application, status) {\r\n      const updatedApplication = { ...application, status };\r\n      this.$request.put('/application/update', updatedApplication).then(res => {\r\n        if (res.data) {\r\n          this.$message.success('Application status updated successfully');\r\n          this.loadApplications();\r\n        } else {\r\n          this.$message.error('Failed to update application status');\r\n        }\r\n      }).catch(error => {\r\n        console.error('Error updating application status:', error);\r\n        this.$message.error('An error occurred while updating the application status');\r\n      });\r\n    },\r\n    handlePageChange(pageNum) {\r\n      this.pageNum = pageNum;\r\n      this.loadApplications();\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.search {\r\n  margin-bottom: 20px;\r\n}\r\n.operation {\r\n  margin-bottom: 20px;\r\n}\r\n.pagination {\r\n  margin-top: 20px;\r\n  text-align: right;\r\n}\r\n.dialog-footer {\r\n  text-align: right;\r\n}\r\n</style>\r\n"],"mappings":"AAoEA;EACAA,IAAA;EACAC,KAAA;IACA;MACAC,YAAA;MACAC,OAAA;MACAC,QAAA;MACAC,KAAA;MACAC,MAAA;QACAN,IAAA;QACAO,MAAA;MACA;MACAC,WAAA;MACAC,IAAA;QACAC,QAAA;QACAC,MAAA;MACA;MACAC,IAAA,EAAAC,IAAA,CAAAC,KAAA,CAAAC,YAAA,CAAAC,OAAA;MACAC,KAAA;QACAP,QAAA,GACA;UAAAQ,QAAA;UAAAC,OAAA;UAAAC,OAAA;QAAA,EACA;QACAT,MAAA,GACA;UAAAO,QAAA;UAAAC,OAAA;UAAAC,OAAA;QAAA;MAEA;IACA;EACA;EACAC,QAAA;IACA,KAAAC,gBAAA;EACA;EACAC,OAAA;IACAD,iBAAA;MACA,KAAAE,QAAA,CAAAC,GAAA;QACAC,MAAA;UACAvB,OAAA,OAAAA,OAAA;UACAC,QAAA,OAAAA,QAAA;UACAJ,IAAA,OAAAM,MAAA,CAAAN,IAAA;UACAO,MAAA,OAAAD,MAAA,CAAAC;QACA;MACA,GAAAoB,IAAA,CAAAC,GAAA;QACA,IAAAA,GAAA,CAAA3B,IAAA;UACA,KAAAC,YAAA,GAAA0B,GAAA,CAAA3B,IAAA,CAAA4B,IAAA;UACA,KAAAxB,KAAA,GAAAuB,GAAA,CAAA3B,IAAA,CAAAI,KAAA;QACA;UACA,KAAAyB,QAAA,CAAAC,KAAA;QACA;MACA,GAAAC,KAAA,CAAAD,KAAA;QACAE,OAAA,CAAAF,KAAA,gCAAAA,KAAA;QACA,KAAAD,QAAA,CAAAC,KAAA;MACA;IACA;IACAG,YAAA;MACA,KAAA5B,MAAA,CAAAN,IAAA;MACA,KAAAM,MAAA,CAAAC,MAAA;MACA,KAAAe,gBAAA;IACA;IACAa,YAAA;MACA,KAAA1B,IAAA;QACAC,QAAA;QACAC,MAAA;MACA;MACA,KAAAH,WAAA;IACA;IACA4B,kBAAA;MACA,KAAAC,KAAA,CAAAC,OAAA,CAAAC,QAAA,CAAAC,KAAA;QACA,IAAAA,KAAA;UACA,KAAAhB,QAAA,CAAAiB,IAAA,0BAAAhC,IAAA,EAAAkB,IAAA,CAAAC,GAAA;YACA,IAAAA,GAAA,CAAA3B,IAAA;cACA,KAAA6B,QAAA,CAAAY,O