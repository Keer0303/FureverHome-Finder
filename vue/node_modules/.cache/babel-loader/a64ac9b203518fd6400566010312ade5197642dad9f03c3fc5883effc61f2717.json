ss=\"manager-header-right\">\r\n        <el-dropdown placement=\"bottom\">\r\n          <div class=\"avatar\">\r\n            <img :src=\"user.avatar || 'https://cube.elemecdn.com/3/7c/3ea6beec64369c2642b92c6726f1epng.png'\" />\r\n            <div>{{ user.name ||  'Admin' }}</div>\r\n          </div>\r\n          <el-dropdown-menu slot=\"dropdown\">\r\n            <el-dropdown-item @click.native=\"goToPerson\">Personal Center</el-dropdown-item>\r\n            <el-dropdown-item @click.native=\"$router.push('/password')\">Change Password</el-dropdown-item>\r\n            <el-dropdown-item @click.native=\"logout\">Logout</el-dropdown-item>\r\n          </el-dropdown-menu>\r\n        </el-dropdown>\r\n      </div>\r\n    </div>\r\n\r\n    <!--  Main  -->\r\n    <div class=\"manager-main\">\r\n      <!--  Sidebar  -->\r\n      <div class=\"manager-main-left\">\r\n        <el-menu :default-openeds=\"['info', 'user']\" router style=\"border: none\" :default-active=\"$route.path\">\r\n          <el-menu-item index=\"/home\">\r\n            <i class=\"el-icon-s-home\"></i>\r\n            <span slot=\"title\">Home</span>\r\n          </el-menu-item>\r\n          <el-submenu index=\"info\">\r\n            <template slot=\"title\">\r\n              <i class=\"el-icon-menu\"></i><span>Information</span>\r\n            </template>\r\n            <el-menu-item index=\"/notice\">Notice Board</el-menu-item>\r\n            <el-menu-item index=\"/animal\">Animal Info</el-menu-item>\r\n            <el-menu-item index=\"/adopt\">Adopt Record</el-menu-item>\r\n            <el-menu-item index=\"/adoptionApplications\">Applications</el-menu-item>\r\n            <el-menu-item index=\"/submit\">{{ submitMenuName }}</el-menu-item>\r\n          </el-submenu>\r\n\r\n          <el-submenu index=\"user\">\r\n            <template slot=\"title\">\r\n              <i class=\"el-icon-menu\"></i><span>User</span>\r\n            </template>\r\n            <el-menu-item index=\"/admin\">Admin Info</el-menu-item>\r\n            <el-menu-item index=\"/user\">User Info</el-menu-item>\r\n          </el-submenu>\r\n        </el-menu>\r\n      </div>\r\n\r\n      <!--  Data Table  -->\r\n      <div class=\"manager-main-right\">\r\n        <router-view @update:user=\"updateUser\" />\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport User from \"@/views/manager/User.vue\";\r\nimport { mapState } from 'vuex';\r\n\r\nexport default {\r\n  name: \"Manager\",\r\n  computed: {\r\n    ...mapState(['user']),\r\n    submitMenuName() {\r\n      return this.user.role === 'ADMIN' ? 'Reports' : 'Report Stray Animal';\r\n    }\r\n  },\r\n  created() {\r\n    if (!this.user.id) {\r\n      this.$router.push('/login')\r\n    }\r\n  },\r\n  methods: {\r\n    updateUser() {\r\n      this.user = JSON.parse(localStorage.getItem('xm-user') || '{}')   // Re-fetch the latest user info\r\n    },\r\n    goToPerson() {\r\n      if (this.user.role === 'ADMIN') {\r\n        this.$router.push('/adminPerson')\r\n      }\r\n      if (this.user.role === 'USER') {\r\n        this.$router.push('/userPerson')\r\n      }\r\n    },\r\n    logout() {\r\n      localStorage.removeItem('xm-user')\r\n      this.$router.push('/login')\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n@import \"@/assets/css/manager.css\";\r\n</style>\r\n"],"mappings":";AAyEA,OAAAA,IAAA;AACA,SAAAC,QAAA;AAEA;EACAC,IAAA;EACAC,QAAA;IACA,GAAAF,QAAA;IACAG,eAAA;MACA,YAAAC,IAAA,CAAAC,IAAA;IACA;EACA;EACAC,QAAA;IACA,UAAAF,IAAA,CAAAG,EAAA;MACA,KAAAC,OAAA,CAAAC,IAAA;IACA;EACA;EACAC,OAAA;IACAC,WAAA;MACA,KAAAP,IAAA,GAAAQ,IAAA,CAAAC,KAAA,CAAAC,YAAA,CAAAC,OAAA;IACA;;IACAC,WAAA;MACA,SAAAZ,IAAA,CAAAC,IAAA;QACA,KAAAG,OAAA,CAAAC,IAAA;MACA;MACA,SAAAL,IAAA,CAAAC,IAAA;QACA,KAAAG,OAAA,CAAAC,IAAA;MACA;IACA;IACAQ,OAAA;MACAH,YAAA,CAAAI,UAAA;MACA,KAAAV,OAAA,CAAAC,IAAA;IACA;EACA;AACA"},"metadata":{},"sourceType":"module","externalDependencies":[]}